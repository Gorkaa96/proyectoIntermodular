<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gorka_hernandez.memorygame">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:icon="@drawable/depositphotos_53310845_stock_illustration_question_mark_sign_icon_help"
        android:label="@string/app_name"
        android:roundIcon="@drawable/depositphotos_53310845_stock_illustration_question_mark_sign_icon_help"
        android:supportsRtl="true"
        android:theme="@style/Theme.MemoryGame">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartActivity">
        </activity>
        <activity android:name=".LevelActivity" android:parentActivityName=".StartActivity">
        </activity>
        <activity android:name=".GameActivity">
        </activity>
        <activity android:name=".PuntuacionesActivity" android:parentActivityName=".StartActivity">
        </activity>
        <activity android:name=".CreditosActivity" android:parentActivityName=".StartActivity">
        </activity>

    </application>

</manifest>